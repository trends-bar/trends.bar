{
    "trendId": "coronavirus",
    "source": "World Health Organization",
    "sourceName": "situation-report",
    "timestamp": "20200312",
    "timestampFormat": "YYYYMMDD",
    "parsers": [
        {
            "title": "Worldwide Cases",
            "regex": {
                "body": "Globally[\\n\\r\\s]*(\\d+\\s*\\d*)\\sconfirmed",
                "flags": "mi"
            }
        },
        {
            "title": "Worldwide Deaths",
            "validFrom": "20200309",
            "regex": {
                "body": "Globally[\\n\\r\\s]+\\d+\\s*\\d*\\sconfirmed[\\n*\\s\\/\\)\\(\\D&-]+\\d+[\\n*\\s\\/\\)\\(\\D&-]+(\\d+\\s*\\d*)",
                "flags": "mi"
            }
        },
        {
            "title": "Worldwide Deaths",
            "validFrom": "20200208",
            "startRegex": {
                "body": "SITUATION IN NUMBERS",
                "flags": "mi"
            },
            "endRegex": {
                "body": "WHO RISK ASSESSMENT",
                "flags": "mi"
            },
            "regex": {
                "body": "(\\d+\\s*\\d*)\\sdeath|death",
                "flags": "gmi"
            }
        },
        {
            "title": "China Cases",
            "regex": {
                "body": "total\\s*\\n*\\d+\\s*\\n*\\d+ \\d+ \\d+ (\\d+) \\d+",
                "flags": "mi"
            }
        },
        {
            "title": "China Deaths",
            "regex": {
                "body": "total\\s*\\n*\\d+\\s*\\n*\\d+ \\d+ \\d+ \\d+ (\\d+)",
                "flags": "mi"
            }
        },
        {
            "title": "Rest of the World Cases",
            "regex": {
                "body": "grand total\\n*\\S*\\n* (\\d+\\s*\\d*) \\d+ \\d+ \\d+",
                "flags": "mi"
            }
        },
        {
            "title": "Rest of the World Deaths",
            "regex": {
                "body": "grand total\\n*\\S*\\n* \\d+\\s*\\d* \\d+ (\\d+) \\d+",
                "flags": "mi"
            }
        }
    ],
    "parsersCombiners": [
        {
            "title": "$country Cases",
            "startRegex": {
                "body": "table 2\\.",
                "flags": "mi"
            },
            "endRegex": {
                "body": "grand total",
                "flags": "mi"
            },
            "regex": {
                "body": "[\\s\\/\\)\\(\\D&-]+(\\d+) \\d+ \\d+ \\d+ \\D+ \\d+",
                "flags": "gmi"
            },
            "postTransform": {
                "transform": "replace",
                "source": "regexp",
                "sourceIndex": 0,
                "valueIndex": 1,
                "dest": "$country",
                "algo": "matchCountryName"
            }
        },
        {
            "title": "$country Deaths",
            "startRegex": {
                "body": "table 2\\.",
                "flags": "mi"
            },
            "endRegex": {
                "body": "grand total",
                "flags": "mi"
            },
            "regex": {
                "body": "[\\s\\/\\)\\(\\D&-]+\\d+ \\d+ (\\d+) \\d+ \\D+ \\d+",
                "flags": "gmi"
            },
            "postTransform": {
                "transform": "replace",
                "source": "regexp",
                "sourceIndex": 0,
                "valueIndex": 1,
                "dest": "$country",
                "algo": "matchCountryName"
            }
        }
    ]
}